{
  "_comment": "Configuration file for JSDoc."
, "tags": {
    "allowUnknownTags": true
  }
, "source": {
    "include": [
      "lib/parse.js"
    , "lib/sort.js"
    , "lib/timestamp.js"
    , "lib/util.js"
    ]
    , "includePattern": ".+\\.js(doc)?$"
    , "excludePattern": "(^|\\/|\\\\)_"
  }
, "plugins": [ "plugins/markdown" ]
, "markdown": {
    "parser": "evilstreak"
}
, "templates": {
    "cleverLinks": true
  , "monospaceLinks": false
  }
}
